if (reason === \"clickaway\") {\n            return;\n        }\n\n        setOpen(false);\n    };\n\n    const getAllAuctions = () => {\n        axios.get(\"https://localhost:5000/auctions\").then((res) => {\n            if (res.status === 200) {\n                setAuctions(res.data);\n            } else {\n                setAlertMessage(\"Błąd ładowania danych\");\n                handleClick()\n            }\n        });\n    };\n\n    useEffect(() => {\n        getAllAuctions();\n    }, []);\n\n    return (\n        <div className={\"auction-list\"}>\n            <h3>Lista aukcji</h3>\n            <Navbar/>\n            <div className={\"buttons\"}>\n                <Stack direction={\"row\"} spacing={\"4px\"} justifyContent={\"center\"}>\n                    <Button variant={\"outlined\"} onClick={() => getAllAuctions()}>Odśwież aukcje</Button>\n                    <Button variant={\"outlined\"} onClick={() => history.push(\"/\")}>\n                        Powrót na strone główną\n                    </Button>\n                </Stack>\n            </div>\n            <SearchBar\n                setAuctions={setAuctions}\n                cancelSearch={getAllAuctions}\n                setAlertMessage={setAlertMessage}\n                handleClick={handleClick}\n            />\n            <div className={\"items\"}>\n\n                {auctions.map((n) => (\n                    <div className={'space-bet'} key={n._id} onClick={() => history.push(`/auctions/${n._id}`)}>\n                        <p>\n                            {n.name}\n                        </p>\n                        <p>\n                            Cena: {n.price}\n                        </p>\n                    </div>\n                ))}\n            </div>\n            <Snackbar\n                open={open}\n                autoHideDuration={6000}\n                onClose={handleClose}\n            >\n                <Alert\n                    onClose={handleClose}\n                    severity=\"warning\"\n                    sx={{width: \"100%\"}}\n                >\n                    {alertMessage}\n                </Alert>\n            </Snackbar>\n        </div>\n    );\n};\n\nexport default AuctionList;\n","/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/AuctionDetails.js",["269","270","271"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/chats/ChatRoomForm.js",["272"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/chats/Chat.js",["273","274"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/chats/DMChat.js",["275","276","277"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/chats/MyDMChats.js",["278"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/auctions/MyAuctions.js",["279"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/auctions/AllAuctions.js",[],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/auctions/AuctionForm.js",["280"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/auctions/AuctionCommentForm.js",["281"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/account/DeleteAccount.js",[],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/account/Account.js",["282","283"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/account/ChangePasswordForm.js",[],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/users/UserDetails.js",["284","285","286"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/users/UserCommentForm.js",["287"],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/app/src/ui/logged/NavbarLogged.js",[],"/home/mtx22/Pulpit/Protokoły sieci Web/stapaj-mateusz/Projekt/a